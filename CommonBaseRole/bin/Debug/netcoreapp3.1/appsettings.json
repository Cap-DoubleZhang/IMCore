{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AppSetting": {
    //数据库配置
    "SQLSetting": {
      //数据库连接字符串
      "ConnectionStr": "Data source=.;initial catalog=IMCore;persist security info=True;user id=sa;password=1234567890;MultipleActiveResultSets=True;App=EntityFramework"
    },
    //密码配置
    "PasswordSetting": {
      //加密方式（0：不加密；1：MD5加密；2：MD5加密后截取）
      "EncryptType": "2",
      //密码截取开始索引
      "SubStart": "6",
      //密码截取位数
      "SubString": "20"
    },
    //Redis配置
    "RedisSetting": {
      //IP地址及端口号
      "IPAddress": "127.0.0.1:6379",
      //密码
      "Password": "qwe00..;",
      //Redis 默认缓存时间（随机小时），避免缓存雪崩。
      "StartTime": "5",
      "EndTime": "30"
    },
    //JWT配置
    "JwtSetting": {
      "SecurityKey": "Fh$g123#3KHDS@djlD%hfUDu*gdhmGhF",
      //发送者
      "issuer": "localhost:5000",
      //接收者
      "audience": "localhost:5000",
      //过期时间(Days)
      "exp": "5"
    },
    "IMCore": {
      "Servers": "127.0.0.1:5000"
    }
  }
}
